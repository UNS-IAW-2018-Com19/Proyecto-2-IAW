{% extends 'layout.twig' %}

{% block estilos %}
     <!-- Estilos propios -->
    <link id="Estilos propios" href="stylesheets/styles.css" rel="stylesheet">
    <link href="stylesheets/content-menu-style.css" rel="stylesheet">
    <link id="estilo" href="stylesheets/redstyle.css" rel="stylesheet">    
{% endblock %}


{% block equipos %}
    <div id="gridEquipos" class = "container-full">
            <div class="row" id="contenedorEquipos"></div>
    </div>
{% endblock %}

{% block fixture %}
    <div id="gridFixture" class"container-full">
         <div class="row" id="contenedorFixture"></div>   
    </div>  
     <!-- Modal -->  
     <div class="modal fade" id="classModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <div class="modal-content" id = "principalModal">
                    <div class="modal-header">
                        <h5 class="modal-title" id="classModalLabel"></h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body" id="modalTable">
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
    </div>  
{% endblock %}


{% block posiciones %}
    <div id="gridTabla" class = "container-full">
        <div class="row" id="tablaPosicion"></div>
    </div>
{% endblock %}

{% block scripts %}
    <script src="javascripts/posiciones.js"></script>
    <script src="javascripts/fixture.js"></script>
    <script src="javascripts/scroll.js"></script>
    <script src="javascripts/cargacomponentes.js"></script>
    <script src="javascripts/listarEquipos.js"></script>
    <script src="javascripts/storage.js"></script>
    <script src="javascripts/cambioTema.js"></script>
{% endblock %}